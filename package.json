    "@types/node": "^9.6.5",
    "@types/node-fetch": "^1.6.8",
    "aws-sdk": "^2.224.1",
    "delay": "^2.0.0",
    "elasticsearch": "^14.2.2",
    "http-aws-es": "^4.0.0",
    "lodash": "^4.17.5",
    "node-fetch": "^2.1.2",
    "uuid": "^3.2.1",
    "w3c-dcat": "^0.3.1"
